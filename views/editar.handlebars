
    <div class="container">
        <h1>Tela de Edição</h1>
        {{#each post}}
                {{#dataValues}}
                <form action="/editar" method="POST">
                
                    <div class="form-group">
                <label class="form-label" for="nome">Nome:</label>
                <input name="nome" type="name" class="form-control" id="nome" placeholder="Digite o nome" value="{{nome}}">
            </div>
            <div class="form-group">
                <label class="form-label" for="nome">Telefone:</label>
                <input name="telefone" type="name" class="form-control" id="telefone" placeholder="Digite o telefone" value="{{telefone}}">
            </div>
            <div class="form-group">
                <label class="form-label" for="endereco">Endereço:</label>
                <input name="endereco" type="name" class="form-control" id="endereco" placeholder="Digite o endereço" value="{{endereco}}">
            </div>
            <div class="form-group">
                <label class="form-label" for="bairro">Bairro:</label>
                <input name="bairro" type="name" class="form-control" id="bairro" placeholder="Digite o bairro" value="{{bairro}}">
            </div>
            <div class="form-group">
                <label class="form-label" for="cep">CEP:</label>
                <input name="cep" type="name" class="form-control" id="cep" placeholder="Digite o CEP" value="{{cep}}">
            </div>
            <div class="form-group">
                <label class="form-label" for="cidade">Cidade:</label>
                <input name="cidade" type="name" class="form-control" id="cidade" placeholder="Digite a cidade" value="{{cidade}}">
            </div>
            <div class="form-group">
                <label class="form-label" for="estado">Estado:</label>
                <input name="estado" type="name" class="form-control" id="estado" placeholder="Digite o estado" value="{{estado}}">
            </div>
                    <button type="submit" class="btn btn-primary">Cadastrar</button>
                </form>
                {{/dataValues}}
            {{/each}}
    </div>